<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link rel="stylesheet" type="text/css" href="~/Content/creativeinspiration.css" />

    <title>@ViewBag.Title</title>

    <link href="@Url.Content("~/Content/sitelogin.css")" rel="stylesheet" type="text/css" />
    @RenderSection("Styles", false)
</head>

<body>
    <div id="page">

        <div id="header">
            <div id="headerTitle">Hermes</div>
            <div id="headerSubText">Painel de gestão para Clientes.</div>
        </div>
        <div class="contentTitle"><h1>@ViewBag.Title</h1></div>
        <div class="contentText">
            @RenderBody()
        </div>
        <div id="footer"></div>
    </div>
    
</body>
</html>
